





if __name__ == "__main__":

    class decorator:
        def __init__(self, func):
            self.func = func

        def __call__(*args, **kwargs):
            print(f"called {self.func.__name__} with {args} and {kwargs}")
            return func(*args, **kwargs)
       


    @decorator
    def func(x,y):
        return x,y



    func(1,2)
