
class Holding:
    def __init__(self, name, date,shares, price):
        self.name = name;
        self.data = date;
        self.shares = shares;
        self.price = price;

    @property
    def price(self):
        return self.price

    @price.setter
    def price(self, newPrice):
        if not isinstance(newPrice, float):
            raise TypeError("Expected float")
        self.price = newPrice

    @property
    def shares(self):
        return self.shares

    @shares.setter
    def shares(self, newShares):
        if not isinstance(newShares, float):
            raise TypeError("Expected float")
        self.shares = newShares





if __name__ == "__main__":
    h = Holding()
    h.price = "sai"
