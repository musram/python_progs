#http://34.212.143.74/s201911/pycon2019/docs/design_patterns.html#_singleton_pattern
class Duck:

    def quack(self):
      print('Quack')

    def fly(self):
        print("I'm flying")


class Turkey:

    def gobble(self):
        print('Gobble gobble')

    def fly(self):
        print("I'm flying a short distance")
