class Node(object):
    def __init__(self, value):
        self.value = value
        self.next = None

    def __repr__(self):
        return self.value



class Queue(object):
    def __init__(self):
        self.front = None
        self.rear = None


    def __repr__(self):
        node = self.rear
        nodes = []
        while self.rear != self.front:
            nodes.append(node.value)
            node = node.next
        return '-->'.join(nodes)

    def enque(self, value):
        
            
