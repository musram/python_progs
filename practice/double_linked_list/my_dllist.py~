class DoubleLinkedListNode(object):
    def __init__(self, value, prev=None, next=None):
        self.value = value
        self.prev = prev
        self.next = next


    def __repr__(self):
        return self.value


class DoubleLinkedList(object):
    def __init__(self):
        self.begin = None
        self.count = 0

    def __repr__(self):
        node = self.begin
        nodes = []
        while node:
            nodes.append(node.value)
            node = node.next

        return '-->'.join(nodes)

    def push(self, value):
        
